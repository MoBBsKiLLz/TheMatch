{
  "expo": {
    "name": "The Match",
    "slug": "the-match",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "thematch",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/foreground.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c3639"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mzepeda.thematch"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreground.png",
        "backgroundColor": "#2c3639"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mzepeda.thematch"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-font",
      "expo-sqlite",
      "@sentry/react-native"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "59ff1b8d-c9da-49c8-a6f1-02c1b62a9168"
      }
    }
  }
}
